{
  "$schema": "vscode://schemas/color-theme",
  "name": "ppy Light",
  "type": "light",
  "colors": {
    "activityBar.background": "#D0D0D0",
    "activityBar.foreground": "#232323",
    "activityBar.inactiveForeground": "#6D6D6D",
    "activityBarBadge.background": "#376388",
    "activityBarBadge.foreground": "#FFFFFF",
    "badge.background": "#C0C0C0",
    "badge.foreground": "#232323",
    "button.background": "#C0C0C0",
    "button.foreground": "#232323",
    "button.hoverBackground": "#B0B0B0",
    "dropdown.background": "#DBDBDB",
    "dropdown.border": "#C0C0C0",
    "dropdown.list.background": "#DBDBDB",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#383A3F",
    "editor.lineHighlightBackground": "#F1F1F1",
    "editor.selectionBackground": "#E5DEFF",
    "editor.wordHighlightBackground": "#E5DEFF99",
    "editor.findMatchBackground": "#D7AB5480",
    "editor.findMatchHighlightBackground": "#D7AB5440",
    "editorCursor.foreground": "#6A6A6A",
    "editorGroup.border": "#E4E4E4",
    "editorGroupHeader.tabsBackground": "#E4E4E4",
    "editorGutter.background": "#FFFFFF",
    "editorHoverWidget.background": "#F1F1F1",
    "editorHoverWidget.border": "#C0C0C0",
    "editorIndentGuide.background": "#D5D5DB",
    "editorIndentGuide.activeBackground": "#3F3F3F",
    "editorLineNumber.foreground": "#8D8F9A",
    "editorLineNumber.activeForeground": "#232323",
    "editorWidget.background": "#DBDBDB",
    "editorWidget.border": "#C0C0C0",
    "focusBorder": "#C0C0C0",
    "foreground": "#232323",
    "input.background": "#DBDBDB",
    "input.border": "#C0C0C0",
    "input.foreground": "#232323",
    "input.placeholderForeground": "#6D6D6D",
    "inputOption.activeBackground": "#C0C0C0",
    "list.activeSelectionBackground": "#C0C0C0",
    "list.activeSelectionForeground": "#1C1C1C",
    "list.focusBackground": "#D0D0D0",
    "list.hoverBackground": "#E4E4E4",
    "list.inactiveSelectionBackground": "#E4E4E4",
    "list.highlightForeground": "#D7AB54",
    "menu.background": "#E4E4E4",
    "menu.foreground": "#232323",
    "menu.selectionBackground": "#C0C0C0",
    "menu.separatorBackground": "#6D6D6D",
    "peekView.border": "#C0C0C0",
    "peekViewEditor.background": "#DBDBDB",
    "peekViewResult.background": "#E4E4E4",
    "peekViewTitle.background": "#C0C0C0",
    "peekViewTitleDescription.foreground": "#6D6D6D",
    "peekViewTitleLabel.foreground": "#232323",
    "progressBar.background": "#8F8F8F",
    "sideBar.background": "#E4E4E4",
    "sideBar.border": "#E4E4E4",
    "sideBar.foreground": "#232323",
    "sideBarSectionHeader.background": "#C0C0C0",
    "sideBarSectionHeader.foreground": "#232323",
    "statusBar.background": "#C0C0C0",
    "statusBar.debuggingBackground": "#A64270",
    "statusBar.noFolderBackground": "#C0C0C0",
    "statusBar.foreground": "#232323",
    "statusBar.border": "#E4E4E4",
    "tab.activeBackground": "#FFFFFF",

    "tab.activeForeground": "#232323",
    "tab.border": "#E4E4E4",
    "tab.inactiveBackground": "#E4E4E4",
    "tab.inactiveForeground": "#6D6D6D",
    "tab.activeBorderTop": "#3F3F3F",
    "titleBar.activeBackground": "#E4E4E4",
    "titleBar.activeForeground": "#232323",
    "titleBar.inactiveBackground": "#E4E4E4",
    "titleBar.inactiveForeground": "#6D6D6D",
    "titleBar.border": "#E4E4E4"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#939AA6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#C57D42"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#A64270"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#CC5450"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#383A3F"
      }
    },
    {
      "name": "Classes and Types",
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#376388"
      }
    },
    {
      "name": "Interfaces",
      "scope": "entity.name.interface",
      "settings": {
        "foreground": "#2978B9"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#71983B"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#4F4F4F"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#23B5FF"
      }
    },
    {
      "name": "Constants & Enums",
      "scope": [
        "constant",
        "variable.other.constant",
        "support.constant",
        "entity.name.enum",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#3B7B9B"
      }
    },
    {
      "name": "Preprocessor",
      "scope": ["meta.preprocessor", "keyword.control.preprocessor"],
      "settings": {
        "foreground": "#A64270",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#4F4F4F"
      }
    },
    {
      "name": "Semicolons",
      "scope": "punctuation.terminator.statement",
      "settings": {
        "foreground": "#BDBDDCA"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#CC5450"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3B739B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A64270"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#A64270"
      }
    }
  ]
}
